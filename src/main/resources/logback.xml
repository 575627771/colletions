<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <contextName>wu.test</contextName>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder charset="UTF-8">
            <pattern>[%date{ISO8601}] [%-5level]-[%thread] [%X{requestId}] [%logger] - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>